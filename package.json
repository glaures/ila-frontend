{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nuxi dev --dotenv .env.development",
    "bump": "node bump-version.js",
    "generate": "nuxt generate --dotenv .env.production && echo jmoosdorf-ila.de > .output/public/CNAME",
    "deploy": "cross-env NODE_ENV=production npm run bump && npm run generate && gh-pages -d .output/public -b gh-pages"
  },
  "dependencies": {
    "@nuxt/content": "^3.5.1",
    "@nuxt/eslint": "^1.3.1",
    "@nuxt/fonts": "^0.11.3",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.6",
    "@pinia/nuxt": "^0.11.2",
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^2.0.8",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "dotenv-cli": "^10.0.0",
    "gh-pages": "^6.3.0"
  }
}
